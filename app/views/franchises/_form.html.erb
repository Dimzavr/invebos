<%= form_with model: @franchise do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title, required: true, minlength: 2, placeholder: "Name franchise" %>
  </div>
  
  <div>
  	  <%= f.label :description %>
      <%= f.text_area :description, required: true, minlength: 2 %>
  </div> 

  <%= f.submit "Submit franchise" %> 
<% end %>